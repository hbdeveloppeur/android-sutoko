<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:viewBindingIgnore="true"
        xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <View
            android:id="@+id/sutoko.trophies.toolbar.patron1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintDimensionRatio="H, 511:110" />

    <ImageView
            android:id="@+id/sutoko.trophies.toolbar.icon"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            app:layout_constraintStart_toEndOf="@id/sutoko.guideline29"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:srcCompat="@drawable/ic_trophy_white"
            android:alpha="0.7"
            tools:ignore="ContentDescription"/>

    <com.example.sutokosharedelements.fonts.RobotoRegular
            android:id="@+id/sutoko.trophies.toolbar.text.title"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@id/sutoko.trophies.toolbar.icon"
            android:layout_marginStart="@dimen/activity_title_margin_start"
            android:layout_marginLeft="@dimen/activity_title_margin_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0.7"
            android:text="@string/sutoko_trophies_title"
            android:textColor="#FFF"
            android:textSize="@dimen/activity_title_size"
            android:letterSpacing="0.07"
            tools:ignore="UnusedAttribute"/>

    <View
            android:id="@+id/sutoko.trophies.toolbar.button.options.hitbox"
            android:layout_width="0dp" android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@id/sutoko.guideline31"
            app:layout_constraintBottom_toTopOf="@id/sutoko.guideline32"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/sutoko.guideline30"
    />

    <ImageView
            android:layout_width="wrap_content" android:layout_height="20dp"
            app:layout_constraintTop_toTopOf="@id/sutoko.trophies.toolbar.button.options.hitbox"
            app:layout_constraintBottom_toBottomOf="@id/sutoko.trophies.toolbar.button.options.hitbox"
            app:layout_constraintStart_toStartOf="@id/sutoko.trophies.toolbar.button.options.hitbox"
            app:layout_constraintEnd_toEndOf="@id/sutoko.trophies.toolbar.button.options.hitbox"
            app:srcCompat="@drawable/ic_more_vert_white"
            android:alpha="0.7"
            tools:ignore="ContentDescription"/>

    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/sutoko.guideline29" app:layout_constraintGuide_percent="0.07"
            android:orientation="vertical"/>
    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/sutoko.guideline30" app:layout_constraintGuide_percent="0.886"
            android:orientation="vertical"/>
    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/sutoko.guideline31" app:layout_constraintGuide_percent="0.145"
            android:orientation="horizontal"/>
    <androidx.constraintlayout.widget.Guideline
            android:layout_width="wrap_content" android:layout_height="wrap_content"
            android:id="@+id/sutoko.guideline32" app:layout_constraintGuide_percent="0.8"
            android:orientation="horizontal"/>


</androidx.constraintlayout.widget.ConstraintLayout>