<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:viewBindingIgnore="true"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:background="#4F000000">

    <include
        android:id="@+id/smsnotification.item"
        android:layout_height="120dp"
        android:layout_width="0dp"
        layout="@layout/inc_notification_preview"
        app:layout_constraintTop_toBottomOf="@id/sutoko.notification.guideline26"
        app:layout_constraintEnd_toStartOf="@id/sutoko.notification.guideline25"
        app:layout_constraintStart_toEndOf="@id/sutoko.notification.guideline23"
        android:visibility="invisible"
        />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sutoko.notification.guideline23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.043" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sutoko.notification.guideline25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.957" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/sutoko.notification.guideline26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.066"/>
</androidx.constraintlayout.widget.ConstraintLayout>