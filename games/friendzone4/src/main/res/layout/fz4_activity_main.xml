                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@id/fz4_mainactivity.name"
                app:layout_constraintTop_toBottomOf="@id/fz4_guideline36"
                tools:ignore="ContentDescription" />


            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fz4_guideline36"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".225" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fz4_guideline37"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent=".775" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fz4_guideline20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".025" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fz4_guideline28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent=".18" />

            <TextView
                android:id="@+id/fz4_mainactivity.name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="2dp"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/main_header_name"
                app:layout_constraintBottom_toTopOf="@+id/fz4_main_guideline18"
                app:layout_constraintStart_toStartOf="@+id/fz4_main_guideline14" />

            <TextView
