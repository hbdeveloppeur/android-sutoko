                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#000000"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                tools:ignore="TooManyViews" />

            <ImageView
                android:id="@+id/fz4_mainactivty.background.filter"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:background="#66000000"
                android:scaleType="centerCrop"
                android:visibility="invisible"
                tools:ignore="ContentDescription" />
        </RelativeLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/fz4_mainactivity.header"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:background="@color/game_header_background"
            app:layout_constraintBottom_toBottomOf="@id/fz4_main_guideline8"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <View
                android:id="@+id/fz4_mainactivity.header.black_filter"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#151515"
                android:visibility="invisible" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fz4_main_guideline14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.216318" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fz4_main_guideline15"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.6020492"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintLeft_toRightOf="@+id/fz4_main_guideline14" />

            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fz4_main_guideline16"
                android:layout_width="wrap_content"
