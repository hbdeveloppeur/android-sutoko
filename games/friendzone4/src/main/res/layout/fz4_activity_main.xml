            <androidx.constraintlayout.widget.Guideline
                android:id="@+id/fz4_main_guideline23"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                app:layout_constraintGuide_percent="0.5" />

            <ImageView
                android:id="@+id/fz4_mainactivity.notification.photo"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                android:adjustViewBounds="false"
                android:scaleType="centerInside"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="@id/fz4_main_guideline19"
                app:layout_constraintTop_toTopOf="parent"
                tools:ignore="ContentDescription" />

            <ImageView
                android:id="@+id/fz4_mainactivity.notification.icon"
                android:layout_width="0dp"
                android:layout_height="15dp"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="8dp"
                app:layout_constraintBottom_toBottomOf="@+id/fz4_mainactivity.notification.photo"
                app:layout_constraintStart_toEndOf="@+id/fz4_mainactivity.notification.photo"
                app:layout_constraintTop_toTopOf="@+id/fz4_mainactivity.notification.photo"
                tools:ignore="ContentDescription" />

            <TextView

                android:id="@+id/fz4_mainactivity.notification.name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginBottom="4dp"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/main_header_name"
                app:layout_constraintBottom_toTopOf="@id/fz4_main_guideline23"
                app:layout_constraintStart_toEndOf="@id/fz4_mainactivity.notification.icon" />

            <TextView
                android:id="@+id/fz4_mainactivity.notification.libelle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="4dp"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/main_header_status"
                app:layout_constraintStart_toEndOf="@id/fz4_mainactivity.notification.icon"
                app:layout_constraintTop_toTopOf="@id/fz4_main_guideline23" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fz4_main_guideline9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.9" />

        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/fz4_main_guideline8"
            android:layout_width="wrap_content"
