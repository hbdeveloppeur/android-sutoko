                android:layout_height="match_parent" />
        </LinearLayout>

        <ImageView
            android:id="@+id/fz4_mainactivity.button.pause"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:adjustViewBounds="true"
            android:paddingBottom="10dp"
            app:layout_constraintBottom_toTopOf="@id/fz4_main_guideline28"
            app:layout_constraintDimensionRatio="W, 1:1"
            app:layout_constraintRight_toLeftOf="@id/fz4_main_guideline27"
            app:layout_constraintTop_toBottomOf="@id/fz4_main_guideline22"
            tools:ignore="ContentDescription" />

        <ImageView
            android:id="@+id/fz4_mainactivity.button.faster.build"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:adjustViewBounds="true"
            android:paddingBottom="10dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toTopOf="@id/fz4_main_guideline30"
            app:layout_constraintDimensionRatio="W, 1:1"
            app:layout_constraintRight_toLeftOf="@id/fz4_main_guideline27"
            app:layout_constraintTop_toBottomOf="@id/fz4_main_guideline29"
            tools:ignore="ContentDescription" />

        <FrameLayout
            android:id="@+id/fz4_mainactivity.filter"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
