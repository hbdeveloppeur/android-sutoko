                    android:id="@+id/fz4_main.contacts.title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:alpha=".6"
                    android:textColor="#5f000000"
                    android:textSize="15sp"
                    app:layout_constraintStart_toEndOf="@id/fz4_guideline39"
                    app:layout_constraintTop_toBottomOf="@id/fz4_guideline43" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/fz4_guideline39"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    app:layout_constraintGuide_percent=".15162" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/fz4_guideline41"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent=".36405" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/fz4_guideline42"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    app:layout_constraintGuide_percent=".617511" />

                <androidx.constraintlayout.widget.Guideline
                    android:id="@+id/fz4_guideline43"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
