            app:layout_constraintEnd_toEndOf="@id/fz4_guideline23"
            app:layout_constraintStart_toStartOf="@id/fz4_guideline22"
            app:layout_constraintTop_toBottomOf="@id/fz4_main_guideline8">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/fz4_mainActivity.recyclerview.fix"
