                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginTop="2dp"
                android:textColor="@color/colorWhite"
                android:textSize="@dimen/main_header_status"
                app:layout_constraintStart_toStartOf="@+id/fz4_main_guideline14"
                app:layout_constraintTop_toTopOf="@+id/fz4_main_guideline18" />

